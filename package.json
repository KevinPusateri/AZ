{
  "name": "matrix-web-fe-tests",
  "version": "1.0.0",
  "description": "Cypress Tests for Matrix Web",
  "main": "index.js",
  "dependencies": {
    "cypress": "^6.7.1",
    "cypress-file-upload": "^5.0.2",
    "express": "^4.17.1",
    "faker": "^5.4.0"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  },
  "scripts": {
    "cypress:open": "./node_modules/.bin/cypress open",
    "nav_homePage": "./node_modules/.bin/cypress run --spec cypress/integration/navigation/mw_navigation_homePage.js --reporter junit --reporter-options \"mochaFile=results/Report_Matrix_Web_Navigazioni_Da_Home_Page.xml,toConsole=true\"",
    "nav_schedaCliente": "./node_modules/.bin/cypress run --spec cypress/integration/navigation/mw_navigation_scheda_cliente.js --reporter junit --reporter-options \"mochaFile=results/Report_Matrix_Web_Navigazioni_Scheda_Cliente.xml,toConsole=true\"",
    "nav_backOffice": "./node_modules/.bin/cypress run --spec cypress/integration/navigation/mw_navigation_backOffice.js --reporter junit --reporter-options \"mochaFile=results/Report_Matrix_Web_Navigazioni_BackOffice.xml,toConsole=true\"",
    "nav_burgerMenuClients": "./node_modules/.bin/cypress run --spec cypress/integration/navigation/mw_navigation_burgerMenuClients.js --reporter junit --reporter-options \"mochaFile=results/Report_Matrix_Web_Navigazioni_BurgerMenu_Clients.xml,toConsole=true\"",
    "nav_clients": "./node_modules/.bin/cypress run --spec cypress/integration/navigation/mw_navigation_clients.js --reporter junit --reporter-options \"mochaFile=results/Report_Matrix_Web_Navigazioni_Clients.xml,toConsole=true\"",
    "nav_mieInfo": "./node_modules/.bin/cypress run --spec cypress/integration/navigation/mw_navigation_mieInfo.js --reporter junit --reporter-options \"mochaFile=results/Report_Matrix_Web_Navigazioni_Mie_Info.xml,toConsole=true\"",
    "nav_news": "./node_modules/.bin/cypress run --spec cypress/integration/navigation/mw_navigation_news.js --reporter junit --reporter-options \"mochaFile=results/Report_Matrix_Web_Navigazioni_News.xml,toConsole=true\"",
    "nav_numbers": "./node_modules/.bin/cypress run --spec cypress/integration/navigation/mw_navigation_numbers.js --reporter junit --reporter-options \"mochaFile=results/Report_Matrix_Web_Navigazioni_Numbers.xml,toConsole=true\"",
    "nav_sales": "./node_modules/.bin/cypress run --spec cypress/integration/navigation/mw_navigation_sales.js --reporter junit --reporter-options \"mochaFile=results/Report_Matrix_Web_Navigazioni_Sales.xml,toConsole=true\"",
	  "cypress:run:navigations": "npm-run-all -s -c nav_homePage nav_schedaCliente nav_backOffice nav_burgerMenuClients nav_clients nav_mieInfo nav_news nav_numbers nav_sales"
  },
  "repository": {
    "type": "git",
    "url": "https://github.developer.allianz.io/az-italy/matrix-web-fe-tests.git"
  },
  "author": "Andrea Oboe",
  "license": "ISC"
}